<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2011/10/nuspec.xsd">
  <metadata minClientVersion="2.5">
    <id>xunit.core</id>
    <version>2.0.0-beta</version>
    <title>xUnit.net [Core Library]</title>
    <authors>James Newkirk,Brad Wilson</authors>
    <description>
      Includes the core unit testing framework from xUnit.net (xunit.core.dll).
      Supported platforms: Desktop .NET 4.5+, Modern Windows 8+, Windows Phone 8+ (Silverlight), Windows Phone 8.1+ (Universal), MonoTouch, MonoAndroid, Portable Libraries (supporting Profile259).
    </description>
    <language>en-US</language>
    <projectUrl>https://github.com/xunit/xunit</projectUrl>
    <iconUrl>https://raw.githubusercontent.com/xunit/media/master/logo-512-transparent.png</iconUrl>
    <licenseUrl>https://raw.githubusercontent.com/xunit/xunit/master/license.txt</licenseUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <references>
      <reference file="xunit.core.dll" />
    </references>
    <dependencies>
      <group>
        <dependency id="xunit.abstractions" version="[2.0.0-beta]" />
        <!-- FOR WHEN WE RTM: -->
        <!--<dependency id="xunit.abstractions" version="[2.0.0,2.1)" />-->
      </group>
      <group targetFramework="Wpa81">
        <dependency id="xunit.abstractions" version="[2.0.0-beta]" />
        <!-- FOR WHEN WE RTM: -->
        <!--<dependency id="xunit.abstractions" version="[2.0.0,2.1)" />-->
        
        <dependency id="Newtonsoft.Json" version="6.0.3" />
      </group>

    </dependencies>
  </metadata>
  <files>
    <!-- Placeholders to prevent other props from being imported on Xamarin -->
    <file src="xunit.core\build\xunit.core.empty.props" target="build\MonoAndroid\xunit.core.props" />
    <file src="xunit.core\build\xunit.core.empty.props" target="build\MonoTouch\xunit.core.props" />
    
    
    <!-- Win8 libs -->
    <file src="xunit.core\build\xunit.core.win8.props" target="build\win8\xunit.core.props" />
    <file src="xunit.core\bin\Release\xunit.core.dll" target="lib\win8\xunit.core.dll" />
    <file src="xunit.core\bin\Release\xunit.core.pdb" target="lib\win8\xunit.core.pdb" />
    <file src="xunit.core\bin\Release\xunit.core.xml" target="lib\win8\xunit.core.xml" />
    <file src="xunit.execution.net45.noappdomain\bin\Release\xunit.execution.dll" target="lib\Win8\xunit.execution.dll" />    
    <file src="xunit.runner.utility.net45.noappdomain\bin\Release\xunit.runner.utility.dll" target="lib\Win8\xunit.runner.utility.dll" />


    <!-- Main PCL libs -->
    <file src="xunit.core\build\xunit.core.props" target="build\portable-net45+win+wpa81+wp80\xunit.core.props" />
    <file src="xunit.core\bin\Release\xunit.core.dll" target="lib\portable-net45+win+wpa81+wp80+monotouch+monoandroid\xunit.core.dll" />
    <file src="xunit.core\bin\Release\xunit.core.pdb" target="lib\portable-net45+win+wpa81+wp80+monotouch+monoandroid\xunit.core.pdb" />
    <file src="xunit.core\bin\Release\xunit.core.xml" target="lib\portable-net45+win+wpa81+wp80+monotouch+monoandroid\xunit.core.xml" />
    <file src="xunit.core\bin\Release\xunit.core.dll.tdnet" target="lib\portable-net45+win+wpa81+wp80+monotouch+monoandroid\xunit.core.dll.tdnet" />
    <file src="xunit.execution.net45\bin\Release\xunit.execution.dll" target="lib\portable-net45+win+wpa81+wp80+monotouch+monoandroid\xunit.execution.dll" />
    <file src="xunit.runner.tdnet\bin\Release\xunit.runner.tdnet.dll" target="lib\portable-net45+win+wpa81+wp80+monotouch+monoandroid\xunit.runner.tdnet.dll" />
    <file src="xunit.runner.utility.net35\bin\Release\xunit.runner.utility.dll" target="lib\portable-net45+win+wpa81+wp80+monotouch+monoandroid\xunit.runner.utility.dll" />
    
    
    <!-- WPA81 -->
    <file src="xunit.core\build\xunit.core.wpa81.props" target="build\wpa81\xunit.core.props" />
    <file src="xunit.core\build\xunit.core.wpa81.targets" target="build\wpa81\xunit.core.targets" />
    <file src="xunit.core\bin\Release\xunit.core.dll" target="lib\wpa81\xunit.core.dll" />
    <file src="xunit.core\bin\Release\xunit.core.pdb" target="lib\wpa81\xunit.core.pdb" />
    <file src="xunit.core\bin\Release\xunit.core.xml" target="lib\wpa81\xunit.core.xml" />
    <file src="xunit.execution.net45.json\bin\Release\xunit.execution.dll" target="lib\wpa81\xunit.execution.dll" />

    <!-- These version of the files are run on the device -->
    <file src="xunit.execution.wpa81\bin\Release\xunit.execution.dll" target="lib\wpa81\WinRT\xunit.execution.dll" />
    <file src="xunit.execution.wpa81\bin\Release\xunit.execution.pri" target="lib\wpa81\WinRT\xunit.execution.pri" />
    <file src="xunit.runner.utility.wpa81\bin\Release\xunit.runner.utility.dll" target="lib\wpa81\WinRT\xunit.runner.utility.dll" />
    <file src="xunit.runner.utility.wpa81\bin\Release\xunit.runner.utility.pri" target="lib\wpa81\WinRT\xunit.runner.utility.pri" />    
    
    
    <!-- Source files -->
    <file src="xunit.core\**\*.cs" target="src\xunit.core" />
    <file src="xunit.assert\Asserts\Sdk\AssertEquality*.cs" target="src\xunit.assert\Asserts\Sdk\" />
    <file src="common\**\*.cs" target="src\common" />
  </files>
</package>