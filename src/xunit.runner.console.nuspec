    <file src="xunit.console\bin\$Configuration$_x86\net46\xunit.console.*"  target="tools\net46\" />

    <file src="xunit.console\bin\$Configuration$\net461\*.dll"               target="tools\net461\" />
    <file src="xunit.console\bin\$Configuration$\net461\xunit.console.*"     target="tools\net461\" />
    <file src="xunit.console\bin\$Configuration$_x86\net461\xunit.console.*" target="tools\net461\" />

    <file src="xunit.console\bin\$Configuration$\net462\*.dll"               target="tools\net462\" />
