<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata minClientVersion="2.12">
    <id>xunit.runner.console</id>
    <version>99.99.99-dev</version>
    <title>xUnit.net [Runner: Console]</title>
    <authors>James Newkirk,Brad Wilson</authors>
    <summary>Console runner for the xUnit.net framework.</summary>
    <description>Console runner for the xUnit.net framework. Capable of running xUnit.net v1.9.2 and v2.0+ tests. Requires .NET 4.5.2 or later.</description>
    <language>en-US</language>
    <projectUrl>https://github.com/xunit/xunit</projectUrl>
    <iconUrl>https://raw.githubusercontent.com/xunit/media/master/logo-512-transparent.png</iconUrl>
    <licenseUrl>https://raw.githubusercontent.com/xunit/xunit/master/license.txt</licenseUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <developmentDependency>true</developmentDependency>
  </metadata>
  <files>
    <file src="xunit.console\*.xslt"                                                        target="tools\" />
    <file src="xunit.console\bin\Release\net452\win7-x64\xunit.abstractions.dll"            target="tools\" />
    <file src="xunit.console\bin\Release\net452\win7-x64\xunit.runner.reporters.net452.dll" target="tools\" />
    <file src="xunit.console\bin\Release\net452\win7-x64\xunit.runner.utility.net452.dll"   target="tools\" />
    <file src="xunit.console\bin\Release\net452\win7-x64\xunit.console.exe"                 target="tools\" />
    <file src="xunit.console\bin\Release\net452\win7-x64\xunit.console.exe.config"          target="tools\" />
    <file src="xunit.console\bin\Release\net452\win7-x64\xunit.console.pdb"                 target="tools\" />
    <file src="xunit.console\bin\Release_x86\net452\win7-x64\xunit.console.x86.exe"         target="tools\" />
    <file src="xunit.console\bin\Release_x86\net452\win7-x64\xunit.console.x86.exe.config"  target="tools\" />
    <file src="xunit.console\bin\Release_x86\net452\win7-x64\xunit.console.x86.pdb"         target="tools\" />

    <file src="common\AssemblyExtensions.cs"   target="src\common\" />
    <file src="common\DictionaryExtensions.cs" target="src\common\" />
    <file src="common\GlobalAssemblyInfo.cs"   target="src\common\" />
    <file src="common\Guard.cs"                target="src\common\" />
    <file src="xunit.console\**\*.cs"          target="src\xunit.console\" />
  </files>
</package>
