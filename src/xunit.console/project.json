{
    "version": "99.99.99-dev",
    "buildOptions": {
        "compile": [
            "../common/AssemblyExtensions.cs",
            "../common/DictionaryExtensions.cs",
            "../common/GlobalAssemblyInfo.cs",
            "../common/Guard.cs",
            "../common/TestDiscoveryVisitor.cs",
            "../common/XmlTestExecutionVisitor.cs"
        ],
        "copyToOutput": [
            "NUnitXml.xslt",
            "xUnit1.xslt",
            "HTML.xslt"
        ],
        "emitEntryPoint": true,
        "platform": "anycpu",
        "warningsAsErrors": true
    },
    "configurations": {
        "Debug_x86": {
            "buildOptions": {
                "outputName": "xunit.console.x86",
                "platform": "x86"
            }
        },
        "Release_x86": {
            "buildOptions": {
                "outputName": "xunit.console.x86",
                "platform": "x86"
            }
        }
    },
    "frameworks": {
        "net45": {
            "frameworkAssemblies": {
                "System.Configuration": "",
                "System.Xml": "",
                "System.Xml.Linq": ""
            }
        }
    },
    "dependencies": {
        "xunit.runner.reporters": "99.99.99-dev"
    }
}
