<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata minClientVersion="2.12">
    <id>xunit.runner.msbuild</id>
    <version>99.99.99-dev</version>
    <title>xUnit.net [Runner: MSBuild]</title>
    <authors>James Newkirk,Brad Wilson</authors>
    <summary>MSBuild runner for the xUnit.net framework.</summary>
    <description>MSBuild runner for the xUnit.net framework. Capable of running xUnit.net v1.9.2 and v2.0+ tests. Requires .NET 4.5.2 or later, or .NET Core 1.0 or later.</description>
    <language>en-US</language>
    <projectUrl>https://github.com/xunit/xunit</projectUrl>
    <iconUrl>https://raw.githubusercontent.com/xunit/media/master/logo-512-transparent.png</iconUrl>
    <licenseUrl>https://raw.githubusercontent.com/xunit/xunit/master/license.txt</licenseUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <developmentDependency>true</developmentDependency>
  </metadata>
  <files>
    <file src="xunit.console\*.xslt"                                                              target="build\net452\" />
    <file src="xunit.runner.msbuild\bin\$Configuration$\net452\xunit.abstractions.dll"            target="build\net452\" />
    <file src="xunit.runner.msbuild\bin\$Configuration$\net452\xunit.runner.reporters.net452.dll" target="build\net452\" />
    <file src="xunit.runner.msbuild\bin\$Configuration$\net452\xunit.runner.utility.net452.dll"   target="build\net452\" />
    <file src="xunit.runner.msbuild\bin\$Configuration$\net452\xunit.runner.msbuild.net452.dll"   target="build\net452\" />
    <file src="xunit.runner.msbuild\build\xunit.runner.msbuild.props"                             target="build\net452\" />
    <file src="xunit.runner.msbuild\build\xunit.runner.msbuild.targets"                           target="build\net452\" />

    <file src="build\_._" target="lib\net452\_._" />

    <file src="xunit.runner.msbuild\bin\$Configuration$\netcoreapp1.0\publish\**\*.*" target="build\netcoreapp1.0\" />
    <file src="xunit.runner.msbuild\build\xunit.runner.msbuild.props"                 target="build\netcoreapp1.0\" />
    <file src="xunit.runner.msbuild\build\xunit.runner.msbuild.targets"               target="build\netcoreapp1.0\" />

    <file src="build\_._" target="lib\netcoreapp1.0\_._" />
  </files>
</package>