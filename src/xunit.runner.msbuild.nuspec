<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata minClientVersion="2.12">
    <id>xunit.runner.msbuild</id>
    <version>$PackageVersion$</version>
    <title>xUnit.net [Runner: MSBuild]</title>
    <authors>jnewkirk,bradwilson</authors>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <license type="expression">Apache-2.0</license>
    <licenseUrl>https://licenses.nuget.org/Apache-2.0</licenseUrl>
    <icon>_content/logo-128-transparent.png</icon>
    <description>MSBuild runner for the xUnit.net framework. Capable of running xUnit.net v1.9.2 and v2.0+ tests. Supports .NET 4.5.2 or later.</description>
    <copyright>Copyright (C) .NET Foundation</copyright>
    <repository type="git" url="https://github.com/xunit/xunit" commit="$GitCommitId$" />
    <developmentDependency>true</developmentDependency>
    <dependencies>
      <group targetFramework="net452" />
    </dependencies>
  </metadata>
  <files>
    <file target="_content\" src="..\tools\media\logo-128-transparent.png" />

    <file src="xunit.console\*.xslt"                                                              target="build\net452\" />
    <file src="xunit.runner.msbuild\bin\$Configuration$\net452\xunit.abstractions.dll"            target="build\net452\" />
    <file src="xunit.runner.msbuild\bin\$Configuration$\net452\xunit.runner.reporters.net452.dll" target="build\net452\" />
    <file src="xunit.runner.msbuild\bin\$Configuration$\net452\xunit.runner.utility.net452.dll"   target="build\net452\" />
    <file src="xunit.runner.msbuild\bin\$Configuration$\net452\xunit.runner.msbuild.net452.dll"   target="build\net452\" />
    <file src="xunit.runner.msbuild\build\xunit.runner.msbuild.props"                             target="build\net452\" />
    <file src="xunit.runner.msbuild\build\xunit.runner.msbuild.targets"                           target="build\net452\" />

    <file src="build\_._" target="lib\net452\_._" />
  </files>
</package>