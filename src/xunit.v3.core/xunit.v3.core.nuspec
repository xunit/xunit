<?xml version="1.0" encoding="utf-8"?>
<package>
	<metadata>
		<id>xunit.v3.core</id>
		<version>$PackageVersion$</version>
		<title>xUnit.net [Core Unit Testing Framework]</title>
		<authors>jnewkirk,bradwilson</authors>
		<requireLicenseAcceptance>false</requireLicenseAcceptance>
		<license type="expression">Apache-2.0</license>
		<licenseUrl>https://licenses.nuget.org/Apache-2.0</licenseUrl>
		<icon>_content/logo-128-transparent.png</icon>
		<description>Includes the core library for writing tests using xUnit.net (xunit.v3.core.dll). Supports .NET Framework 4.7.2 or later and .NET Core 3.1 or later.</description>
		<copyright>Copyright (C) .NET Foundation</copyright>
		<repository type="git" url="https://github.com/xunit/xunit" commit="$GitCommitId$" />
		<dependencies>
			<group targetFramework=".NETCoreApp3.1">
				<dependency id="xunit.v3.extensibility.core" version="[$PackageVersion$]" />
				<dependency id="xunit.v3.runner.inproc.console" version="[$PackageVersion$]" />
			</group>
			<group targetFramework=".NETFramework4.7.2">
				<dependency id="xunit.v3.extensibility.core" version="[$PackageVersion$]" />
				<dependency id="xunit.v3.runner.inproc.console" version="[$PackageVersion$]" />
			</group>
		</dependencies>
	</metadata>
	<files>
		<file target="_content\" src="..\..\tools\media\logo-128-transparent.png" />
		<file target="_content\cs\" src="Package\content\*.cs" />
		<file target="_content\fs\" src="Package\content\*.fs" />
		<file target="_content\vb\" src="Package\content\*.vb" />

		<file target="build\" src="Package\build\xunit.v3.core.*" />

		<file target="lib\net472\_._" src="..\build\_._" />

		<file target="lib\netcoreapp3.1\_._" src="..\build\_._" />
	</files>
</package>
