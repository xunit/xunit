<?xml version="1.0" encoding="utf-8"?>
<package>
	<metadata>
		<id>xunit.v3.extensibility.core</id>
		<version>$PackageVersion$</version>
		<title>xUnit.net v3 [Core Unit Testing Framework Extensibility]</title>
		<authors>jnewkirk,bradwilson</authors>
		<requireLicenseAcceptance>false</requireLicenseAcceptance>
		<license type="expression">Apache-2.0</license>
		<licenseUrl>https://licenses.nuget.org/Apache-2.0</licenseUrl>
		<icon>_content/logo-128-transparent.png</icon>
		<readme>_content/README.md</readme>
		<releaseNotes>https://xunit.net/releases/v3/$PackageVersion$</releaseNotes>
		<description>Includes the core library (xunit.v3.core.dll), to be used for extensibility; test authors should include xunit.v3.core instead. Supports .NET Standard 2.0.</description>
		<copyright>Copyright (C) .NET Foundation</copyright>
		<repository type="git" url="https://github.com/xunit/xunit" commit="$GitCommitId$" />
		<dependencies>
			<group targetFramework="net472">
				<dependency id="Microsoft.Bcl.AsyncInterfaces" version="$MicrosoftBclAsyncInterfacesVersion$" />
				<dependency id="System.Threading.Tasks.Extensions" version="$SystemThreadingTasksExtensionsVersion$" />
				<dependency id="xunit.v3.common" version="[$PackageVersion$]" />
			</group>
			<group targetFramework="netstandard2.0">
				<dependency id="Microsoft.Bcl.AsyncInterfaces" version="$MicrosoftBclAsyncInterfacesVersion$" />
				<dependency id="System.Threading.Tasks.Extensions" version="$SystemThreadingTasksExtensionsVersion$" />
				<dependency id="xunit.v3.common" version="[$PackageVersion$]" />
			</group>
		</dependencies>
		<references>
			<group targetFramework="net472">
				<reference file="xunit.v3.core.dll" />
				<reference file="xunit.v3.core.xml" />
			</group>
			<group targetFramework="netstandard2.0">
				<reference file="xunit.v3.core.dll" />
				<reference file="xunit.v3.core.xml" />
			</group>
		</references>
	</metadata>
	<!-- Remember to update tools\builder\targets\SignAssemblies.cs when assemblies are added or removed -->
	<files>
		<file target="_content\" src="..\..\tools\media\logo-128-transparent.png" />
		<file target="_content\" src="..\..\README.md" />
		<!--
		<file target="_content\tdnet\" src="bin\$Configuration$\netstandard2.0\xunit.v3.core.dll.tdnet" />
		<file target="_content\tdnet\" src="..\xunit.v3.runner.tdnet\bin\$Configuration$\netstandard2.0\merged\$SignedPath$xunit.v3.runner.tdnet.dll" />
		-->

		<file target="build\" src="Package\build\xunit.v3.extensibility.core.*" />

		<file target="lib\netstandard2.0\" src="bin\$Configuration$\netstandard2.0\$SignedPath$xunit.v3.core.dll" />
		<file target="lib\netstandard2.0\" src="bin\$Configuration$\netstandard2.0\xunit.v3.core.xml" />

		<file target="ref\net472\" src="obj\$Configuration$\net472\ref\$SignedPath$xunit.v3.core.dll" />
		<file target="ref\net472\" src="bin\$Configuration$\net472\xunit.v3.core.xml" />

		<file target="ref\netstandard2.0\" src="obj\$Configuration$\netstandard2.0\ref\$SignedPath$xunit.v3.core.dll" />
		<file target="ref\netstandard2.0\" src="bin\$Configuration$\netstandard2.0\xunit.v3.core.xml" />
	</files>
</package>
