        <file src="xunit.runner.msbuild\bin\Release\xunit.runner.msbuild.dll" target="tools\xunit.runner.msbuild.dll" />
        <file src="xunit.runner.utility.net35\bin\Release\xunit.runner.utility.dll" target="tools\xunit.runner.utility.dll" />
