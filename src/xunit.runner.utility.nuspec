<?xml version="1.0" encoding="utf-8"?>
<package>
  <metadata minClientVersion="2.12">
    <id>xunit.runner.utility</id>
    <version>99.99.99-dev</version>
    <title>xUnit.net [Runner Utility]</title>
    <authors>James Newkirk,Brad Wilson</authors>
    <summary>Includes the version-independent runner for xUnit.net (xunit.runner.utility.*.dll).</summary>
    <description>Includes the version-independent runner for xUnit.net to run both v1.9.2 and v2.0+ tests (xunit.runner.utility.*.dll). Supports .NET 3.5, .NET 4.5.2 or later, .NET Standard 1.1, .NET Standard 1.5, UWP, and .NET Core 1.0 or later.</description>
    <language>en-US</language>
    <projectUrl>https://github.com/xunit/xunit</projectUrl>
    <iconUrl>https://raw.githubusercontent.com/xunit/media/master/logo-512-transparent.png</iconUrl>
    <licenseUrl>https://raw.githubusercontent.com/xunit/xunit/master/license.txt</licenseUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <dependencies>
      <group targetFramework="net35">
        <dependency id="xunit.abstractions" version="2.0.2-rc.1" />
      </group>
      <group targetFramework="netstandard1.1">
        <dependency id="NETStandard.Library" version="1.6.1" />
        <dependency id="xunit.abstractions" version="2.0.2-rc.1" />
      </group>
      <group targetFramework="netstandard1.5">
        <dependency id="NETStandard.Library" version="1.6.1" />
        <dependency id="System.Reflection.TypeExtensions" version="4.3.0" />
        <dependency id="xunit.abstractions" version="2.0.2-rc.1" />
      </group>
      <group targetFramework="netstandard2.0">
        <dependency id="System.Reflection.TypeExtensions" version="4.4.0" />
        <dependency id="xunit.abstractions" version="2.0.2-rc.1" />
      </group>
      <group targetFramework="netcoreapp1.0">
        <dependency id="NETStandard.Library" version="1.6.0" />
        <dependency id="System.Runtime.Loader" version="4.0.0" />
        <dependency id="xunit.abstractions" version="2.0.2-rc.1" />
      </group>
      <group targetFramework="uap10.0">
        <dependency id="Microsoft.NETCore.UniversalWindowsPlatform" version="5.2.3" />
        <dependency id="xunit.abstractions" version="2.0.2-rc.1" />
      </group>
    </dependencies>
  </metadata>
  <files>
    <file src="xunit.runner.utility\bin\$Configuration$\net35\xunit.runner.utility.net35.dll" target="lib\net35\" />
    <file src="xunit.runner.utility\bin\$Configuration$\net35\xunit.runner.utility.net35.xml" target="lib\net35\" />

    <file src="xunit.runner.utility\bin\$Configuration$\net452\xunit.runner.utility.net452.dll" target="lib\net452\" />
