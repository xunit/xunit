<?xml version="1.0" encoding="utf-8"?>
<package>
  <metadata minClientVersion="2.12">
    <id>xunit.runner.utility</id>
    <version>99.99.99-dev</version>
    <title>xUnit.net [Runner Utility]</title>
    <authors>James Newkirk,Brad Wilson</authors>
    <summary>Includes the version-independent runner for xUnit.net (xunit.runner.utility.*.dll).</summary>
    <description>Includes the version-independent runner for xUnit.net to run both v1.9.2, v2.x, and v3.x tests (xunit.runner.utility.*.dll). Supports .NET 4.7.2 or later and .NET Standard 2.0 or later.</description>
    <language>en-US</language>
    <projectUrl>https://github.com/xunit/xunit</projectUrl>
    <iconUrl>https://raw.githubusercontent.com/xunit/media/master/logo-512-transparent.png</iconUrl>
    <licenseUrl>https://raw.githubusercontent.com/xunit/xunit/master/LICENSE</licenseUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <dependencies>
      <group targetFramework="net472">
        <dependency id="xunit.abstractions" version="2.0.3" />
      </group>
      <group targetFramework="netstandard2.0">
        <dependency id="NETStandard.Library" version="2.0.3" />
        <dependency id="xunit.abstractions" version="2.0.3" />
      </group>
    </dependencies>
  </metadata>
  <files>
    <file src="xunit.runner.utility\bin\$Configuration$\net472\xunit.runner.utility.net472.dll" target="lib\net472\" />
    <file src="xunit.runner.utility\bin\$Configuration$\net472\xunit.runner.utility.net472.xml" target="lib\net472\" />

    <file src="xunit.runner.utility\bin\$Configuration$\netstandard2.0\xunit.runner.utility.netstandard20.dll" target="lib\netstandard2.0\" />
    <file src="xunit.runner.utility\bin\$Configuration$\netstandard2.0\xunit.runner.utility.netstandard20.xml" target="lib\netstandard2.0\" />
  </files>
</package>