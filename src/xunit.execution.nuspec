<?xml version="1.0" encoding="utf-8"?>
<package>
  <metadata>
    <id>xunit.execution</id>
    <version>2.0.0-beta</version>
    <title>xUnit.net [Test Execution Library]</title>
    <authors>James Newkirk, Brad Wilson</authors>
    <description>
      Includes the execution framework for xUnit.net (xunit.execution.dll).
      Supported platforms: Desktop .NET 4.5+, Modern Windows 8+, Windows Phone 8.1+ (Universal), MonoTouch, MonoAndroid, Portable Libraries (supporting Profile259).
    </description>
    <language>en-US</language>
    <projectUrl>https://github.com/xunit/xunit</projectUrl>
    <iconUrl>https://raw.githubusercontent.com/xunit/media/master/logo-512-transparent.png</iconUrl>
    <licenseUrl>https://raw.githubusercontent.com/xunit/xunit/master/license.txt</licenseUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <dependencies>
      <dependency id="xunit.abstractions" version="[2.0.0-beta]" />
      <!-- FOR WHEN WE RTM: -->
      <!--<dependency id="xunit.abstractions" version="[2.0.0-alpha,2.1)" />-->
      <dependency id="xunit.core" version="[2.0.0-beta]" />
    </dependencies>
  </metadata>
  <files>
    <file src="xunit.execution.net45\bin\Release\xunit.execution.dll"      target="lib\net45\xunit.execution.dll" />
    <file src="xunit.execution.net45\bin\Release\xunit.execution.pdb"      target="lib\net45\xunit.execution.pdb" />
    <file src="xunit.execution.net45\bin\Release\xunit.execution.xml"      target="lib\net45\xunit.execution.xml" />
    <file src="xunit.execution.iOS\bin\iPhone\Release\xunit.execution.dll" target="lib\monotouch\xunit.execution.dll" />
    <file src="xunit.execution.iOS\bin\iPhone\Release\xunit.execution.pdb" target="lib\monotouch\xunit.execution.pdb" />
    <file src="xunit.execution.iOS\bin\iPhone\Release\xunit.execution.xml" target="lib\monotouch\xunit.execution.xml" />
    <file src="xunit.execution.android\bin\Release\xunit.execution.dll"    target="lib\monoandroid\xunit.execution.dll" />
    <file src="xunit.execution.android\bin\Release\xunit.execution.pdb"    target="lib\monoandroid\xunit.execution.pdb" />
    <file src="xunit.execution.android\bin\Release\xunit.execution.xml"    target="lib\monoandroid\xunit.execution.xml" />
    <file src="xunit.execution.wpa81\bin\Release\xunit.execution.dll"      target="lib\portable-wpa81+win81\xunit.execution.dll" />
    <file src="xunit.execution.wpa81\bin\Release\xunit.execution.pdb"      target="lib\portable-wpa81+win81\xunit.execution.pdb" />
    <file src="xunit.execution.wpa81\bin\Release\xunit.execution.xml"      target="lib\portable-wpa81+win81\xunit.execution.xml" />
    <file src="xunit.execution.wpa81\bin\Release\xunit.execution.pri"      target="lib\portable-wpa81+win81\xunit.execution.pri" />

    <!-- Source -->
    <file src="xunit.execution\**\*.cs" target="src\xunit.execution" />
    <file src="xunit.execution.android\**\*.cs" target="src\xunit.execution.android" />
    <file src="xunit.execution.iOS\**\*.cs" target="src\xunit.execution.iOS" />
    <file src="xunit.execution.net45\**\*.cs" target="src\xunit.execution.net45" />
    <file src="xunit.execution.wpa81\**\*.cs" target="src\xunit.execution.wpa81" />
    <file src="xunit.assert\Asserts\Sdk\AssertEquality*.cs" target="src\xunit.assert\Asserts\Sdk\" />
    <file src="messages\**\*.cs" target="src\messages" />
    <file src="common\**\*.cs" target="src\common" />
  </files>
</package>