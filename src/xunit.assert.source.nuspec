<?xml version="1.0" encoding="utf-8"?>
<package>
  <metadata minClientVersion="2.12">
    <id>xunit.assert.source</id>
    <version>99.99.99-dev</version>
    <title>xUnit.net [Assertion Library, as source]</title>
    <authors>James Newkirk,Brad Wilson</authors>
    <summary>Includes the current assertion library from xUnit.net, as source into your project.</summary>
    <description>Includes the current assertion library from xUnit.net, as source into your project. Supports any platform(s) compatible with .NET Standard 1.1.</description>
    <language>en-US</language>
    <projectUrl>https://github.com/xunit/xunit</projectUrl>
    <iconUrl>https://raw.githubusercontent.com/xunit/media/master/logo-512-transparent.png</iconUrl>
    <licenseUrl>https://raw.githubusercontent.com/xunit/xunit/master/LICENSE</licenseUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <developmentDependency>true</developmentDependency>
    <contentFiles>
      <files include="contentFiles/cs/netstandard2.0/Asserts/**/*.cs" buildAction="Compile" />
    </contentFiles>
  </metadata>
  <files>
    <file src="xunit.assert\Asserts\**\*.cs" target="contentFiles\cs\netstandard2.0\Asserts" />
  </files>
</package>